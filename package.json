{
	"name": "eslint-config-neon",
	"version": "0.1.40",
	"description": "The ultimate ESLint shareable config",
	"scripts": {
		"lint": "prettier --check . && eslint . --ext js",
		"format": "prettier --write . && eslint . --ext js --fix",
		"fmt": "yarn format",
		"postinstall": "is-ci || husky install",
		"prepack": "pinst --disable",
		"postpack": "pinst --enable",
		"update": "yarn upgrade-interactive"
	},
	"type": "commonjs",
	"main": "./common.js",
	"files": [
		"*.js",
		"src/*.js"
	],
	"keywords": [
		"eslint",
		"config",
		"typescript",
		"eslintconfig",
		"lint",
		"linter",
		"validate"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/iCrawl/eslint-config-neon.git"
	},
	"bugs": {
		"url": "https://github.com/iCrawl/eslint-config-neon/issues"
	},
	"author": "iCrawl <icrawltogo@gmail.com>",
	"license": "Apache-2.0",
	"dependencies": {
		"@angular-eslint/eslint-plugin": "^14.4.0",
		"@angular-eslint/eslint-plugin-template": "^14.4.0",
		"@angular-eslint/template-parser": "^14.4.0",
		"@next/eslint-plugin-next": "^13.0.4",
		"@rushstack/eslint-patch": "^1.2.0",
		"@typescript-eslint/eslint-plugin": "^5.44.0",
		"@typescript-eslint/parser": "^5.44.0",
		"astro-eslint-parser": "^0.9.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-astro": "^0.21.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-import": "npm:eslint-plugin-i@latest",
		"eslint-plugin-jest": "^27.1.5",
		"eslint-plugin-jsdoc": "^39.6.2",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-lodash": "^7.4.0",
		"eslint-plugin-n": "^15.5.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.31.11",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-rxjs": "^5.0.2",
		"eslint-plugin-rxjs-angular": "^2.0.0",
		"eslint-plugin-sonarjs": "^0.16.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"eslint-plugin-tsdoc": "^0.2.17",
		"eslint-plugin-typescript-sort-keys": "^2.1.0",
		"eslint-plugin-unicorn": "^45.0.0",
		"eslint-plugin-vue": "^9.7.0",
		"vue-eslint-parser": "^9.1.0"
	},
	"devDependencies": {
		"eslint": "^8.28.0",
		"husky": "^8.0.2",
		"is-ci": "^3.0.1",
		"pinst": "^3.0.0",
		"prettier": "^2.7.1",
		"typescript": "^4.9.3"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"packageManager": "yarn@3.2.4"
}
