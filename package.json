{
	"name": "eslint-config-neon",
	"version": "0.1.57",
	"description": "The ultimate ESLint shareable config",
	"scripts": {
		"lint": "prettier --check . && eslint *.js",
		"format": "prettier --write . && eslint *.js --fix",
		"fmt": "yarn format",
		"postinstall": "is-ci || husky install",
		"prepack": "pinst --disable",
		"postpack": "pinst --enable",
		"update": "yarn upgrade-interactive"
	},
	"type": "commonjs",
	"main": "./common.js",
	"files": [
		"*.js",
		"src/*.js",
		"flat/*.js"
	],
	"keywords": [
		"eslint",
		"config",
		"typescript",
		"eslintconfig",
		"lint",
		"linter",
		"validate"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/iCrawl/eslint-config-neon.git"
	},
	"bugs": {
		"url": "https://github.com/iCrawl/eslint-config-neon/issues"
	},
	"author": "iCrawl <icrawltogo@gmail.com>",
	"license": "Apache-2.0",
	"dependencies": {
		"@angular-eslint/eslint-plugin": "^16.2.0",
		"@angular-eslint/eslint-plugin-template": "^16.2.0",
		"@angular-eslint/template-parser": "^16.2.0",
		"@next/eslint-plugin-next": "^13.5.4",
		"@rushstack/eslint-patch": "^1.5.1",
		"@typescript-eslint/eslint-plugin": "^6.7.4",
		"@typescript-eslint/parser": "^6.7.4",
		"astro-eslint-parser": "^0.16.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-mdx": "^2.2.0",
		"eslint-plugin-astro": "^0.29.1",
		"eslint-plugin-cypress": "^2.15.1",
		"eslint-plugin-import": "npm:eslint-plugin-i@latest",
		"eslint-plugin-jsdoc": "^46.8.2",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-mdx": "^2.2.0",
		"eslint-plugin-n": "^16.1.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-rxjs": "^5.0.3",
		"eslint-plugin-rxjs-angular": "^2.0.1",
		"eslint-plugin-sonarjs": "^0.21.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"eslint-plugin-tsdoc": "^0.2.17",
		"eslint-plugin-typescript-sort-keys": "^3.0.0",
		"eslint-plugin-unicorn": "^48.0.1",
		"eslint-plugin-vue": "^9.17.0",
		"globals": "^13.22.0",
		"vue-eslint-parser": "^9.3.1"
	},
	"devDependencies": {
		"eslint": "^8.50.0",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"pinst": "^3.0.0",
		"prettier": "^3.0.3",
		"typescript": "^5.2.2"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"packageManager": "yarn@3.6.2"
}
